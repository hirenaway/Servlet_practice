<?xml version="1.0" encoding="UTF-8"?>
<web-app 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
    id="WebApp_ID" 
    version="3.1">

    <welcome-file-list>
        <welcome-file>URLFile.html</welcome-file>
    </welcome-file-list>

    <servlet>  
        <servlet-name>URL</servlet-name>  
        <servlet-class>com.forprac.URLWriting</servlet-class>  
    </servlet>  
  
    <servlet-mapping>  
        <servlet-name>URL</servlet-name>  
        <url-pattern>/URL</url-pattern>  
    </servlet-mapping> 
    
    <servlet>  
        <servlet-name>URLSecond</servlet-name> 
        <servlet-class>com.forprac.URLWritingSecond</servlet-class>  
    </servlet>  
  
    <servlet-mapping>  
        <servlet-name>URLSecond</servlet-name>  
        <url-pattern>/URLSecond</url-pattern>  
    </servlet-mapping> 
</web-app>